{
    "name": "eslint-plugin-icon-button",
    "version": "1.0.0",
    "description": "Eslint plugin for icon buttons. Makes sure that icon buttons that contain only icons have aria-labels",
    "keywords": [
        "eslint",
        "icon button",
        "small improvements",
        "accessibility",
        "a11y"
    ],
    "main": "index.js",
    "directories": {
        "lib": "lib",
        "test": "tests"
    },
    "scripts": {
        "test": "mocha ./tests/**/*.js"
    },
    "author": "Charisse De Torres",
    "license": "MIT",
    "repository": "SmallImprovements-OpenSource/eslint-plugin-icon-button",
    "dependencies": {
        "jsx-ast-utils": "^2.0.1"
    },
    "devDependencies": {
        "eslint": "^5.8.0",
        "mocha": "^5.2.0"
    },
    "peerDependencies": {
        "eslint": ">=0.8.0"
    }
}
